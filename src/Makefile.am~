mod_skeleton_la_SOURCES = mod_mediaplayer.c mod_mediaplayer.h
mod_skeleton_la_CFLAGS = -Wall ${MODULE_CFLAGS}
mod_skeleton_la_LDFLAGS = -rpath ${AP_LIBEXECDIR} -module -avoid-version ${MODULE_LDFLAGS}

mod_LTLIBRARIES = mod_mediaplayer.la
moddir=${AP_LIBEXECDIR}

install: install-am
	rm -f $(DESTDIR)${AP_LIBEXECDIR}/mod_mediaplayer.a
	rm -f $(DESTDIR)${AP_LIBEXECDIR}/mod_mediaplayer.la
